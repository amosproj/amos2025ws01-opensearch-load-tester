{
  "size": 50,
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "dssCustomMetadataDuo.positions.costCenter1.raw": "{{cost_center_1}}"
          }
        },
        {
          "range": {
            "dssCustomMetadataDuo.positions.serviceDate": {
              "gte": "{{date_from}}",
              "lte": "{{date_to}}"
            }
          }
        }
      ]
    }
  },
  "_source": [
    "dssDocumentName",
    "dssCustomMetadataDuo.invoiceNumber",
    "dssCustomMetadataDuo.invoiceDate",
    "dssCustomMetadataDuo.positions.costCenter1",
    "dssCustomMetadataDuo.positions.serviceDate",
    "dssCustomMetadataDuo.totalGrossAmount",
    "dssCustomMetadataDuo.bookingState"
  ]
}

