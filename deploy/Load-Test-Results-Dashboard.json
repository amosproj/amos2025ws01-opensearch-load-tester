      "datasource": { "type": "loki", "name": "Loki", "uid": "" },
      "targets": [
        {
          "expr": "sum by (http_status_code) (count_over_time({job=\"opensearch-load-tester\"} |= \"${search}\" | json | query_type =~ \"${query_type:regex}\" [$__range]))",
          "legendFormat": "{{http_status_code}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
