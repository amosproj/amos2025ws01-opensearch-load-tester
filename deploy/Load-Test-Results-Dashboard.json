    {
      "title": "Total Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 4 },
      "datasource": { "type": "loki", "name": "Loki", "uid": "" },
      "targets": [
        {
          "expr": "sum(count_over_time({job=\"opensearch-load-tester\"} |= \"${search}\" | json | query_type =~ \"${query_type:regex}\" [$__range]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "color": { "mode": "fixed", "fixedColor": "blue" }, "unit": "short" }
      },
      "options": { "colorMode": "background", "graphMode": "none", "justifyMode": "center" }
    },
    {
      "title": "HTTP Status Distribution",
